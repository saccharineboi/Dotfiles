[user]
	name = omar
    email = saccharineboi@gmail.com
	signingkey = 4F86E01E141499FF6DA92A998C1706A4C623BD97
[core]
	editor = vim
    symlinks = true
[init]
	defaultBranch = main
[diff]
	tool = vimdiff
	submodule = log
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	sw = switch
	me = merge
	fe = fetch
	res = restore
	rem = remote
	reb = rebase
	unstage = restore --staged
	last = log -1 -p --stat
[commit]
	gpgsign = true
[pull]
	rebase = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
